program SubLabII;

{$APPTYPE CONSOLE}

uses
  SysUtils,
  Windows;

Var first, second, third:Real;

begin
	SetConsoleCP(1251);
	SetConsoleOutPutCP(1251);
	write('>>¬ведите первое число! '); readln(first);
	write('>>¬ведите второе число! '); readln(second);
	write('>>¬ведите третье число! '); readln(third);
	if((first<second) and (second<third)) then writeln('”словие выполн€етс€.')
	else writeln('”словие не выполн€етс€.');
	Readln;
end.

